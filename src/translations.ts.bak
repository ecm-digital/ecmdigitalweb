export function t(lang: Lang, key: string, params?: Record<string, any>): string {
    let str = translations[lang]?.[key] || translations.pl?.[key] || key;
    if (params) {
        Object.entries(params).forEach(([k, v]) => {
            str = str.replace(new RegExp(`\\{${k}\\}`, 'g'), String(v));
        });
    }
    return str;
}

